cd /tmp
wget http://share.grandou.net/debian/nvflash/nvflash_20110628-2_all.deb 
sudo dpkg -i nvflash_20110628-2_all.deb

nvflash --bl /usr/lib/nvflash/fastboot.bin --sync

//copy ubuntu-12.10-preinstalled-desktop-armhf+ac100.bootimg u.bootimg
nvflash -r --download 6 /tmp/u.bootimg


//enable NVIDIA corp. APX
//dissable Usb flash
//reboot ac100 by holding powerbtn (twice)
// ...formating /dev/mmcblk0p7


sudo apt-get install abootimg

sudo apt-get update && sudo apt-get upgrade && sudo apt-get dist-upgrade

//http://avedo.net/307/installing-ubuntu-12-10-on-toshiba-ac100/

